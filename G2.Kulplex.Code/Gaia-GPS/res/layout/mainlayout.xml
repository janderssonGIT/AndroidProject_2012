<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    
    

   <com.kulplex.gaia.CustomMapView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000" />
  
    
<FrameLayout
    android:id="@+id/frameLayout1"
    android:layout_width="100dp"
    android:layout_height="240dp"
    android:layout_gravity="right"
    android:layout_marginTop="90dp" >

    		<SlidingDrawer
         	android:id="@+id/sideDrawer"
            android:layout_width="100dp"
        	android:layout_height="fill_parent"
       		android:content="@+id/content"
           	android:handle="@+id/handle1" 
          	android:orientation="horizontal" >
            

           				 <ImageView
               			 android:id="@+id/handle1"
               			 android:layout_width="wrap_content"
               			 android:layout_height="wrap_content"
               			 android:src="@drawable/handler_small" />



            							<AbsoluteLayout
            							    android:id="@+id/content"
            							    android:layout_width="48dp"
            							    android:layout_height="fill_parent"
            							    android:background="@drawable/rightbackground"
            							    android:orientation="horizontal">

            								<ImageView
            								    android:id="@+id/zoom_in"
            								    android:layout_width="wrap_content"
            								    android:layout_height="wrap_content"
            								    android:layout_x="25dp"
            								    android:layout_y="30dp"
            								    android:src="@drawable/zoom_in_50" />

            								<ImageView
            								    android:id="@+id/zoom_out"
            								    android:layout_width="wrap_content"
            								    android:layout_height="wrap_content"
            								    android:layout_x="25dp"
            								    android:layout_y="80dp"
            								    android:src="@drawable/zoom_out_50" />

            								<ImageView
            								    android:id="@+id/actual_location"
            								    android:layout_width="wrap_content"
            								    android:layout_height="wrap_content"
            								    android:layout_x="25dp"
            								    android:layout_y="130dp"/>
            								
            								<ImageView
            								    android:id="@+id/addPOI"
            								    android:layout_width="wrap_content"
            								    android:layout_height="wrap_content"
            								    android:layout_x="25dp"
            								    android:layout_y="180dp"
            								    android:src="@drawable/info_50" />
            								
            								
            								

            								
          			
           		 </AbsoluteLayout>
        		</SlidingDrawer>
        		</FrameLayout>
        		
<FrameLayout
    android:id="@+id/frameLayout2"
    android:layout_width="fill_parent"
    android:layout_height="505dp"
    android:layout_gravity="bottom" >

<SlidingDrawer
    android:id="@+id/mainDrawer"
    android:layout_width="match_parent"
    android:layout_height="407dp"
    android:content="@+id/content2"
    android:handle="@+id/handle2" >

    	    <ImageView
    	        android:id="@+id/handle2"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:src="@drawable/opendrawerbigger" />


    	    <FrameLayout
    	        android:id="@+id/content2"
    	        android:layout_width="match_parent"
    	        android:layout_height="match_parent" 
    	        android:background="@drawable/drawerbackground_blue">

    	        <ImageView
    	            android:id="@+id/planner"
    	            android:layout_width="wrap_content"
    	            android:layout_height="wrap_content"
    	            android:layout_marginLeft="60dp"
    	            android:layout_marginTop="50dp"
    	            android:src="@drawable/blue_travelp_b_150" />

    	        <ImageView
    	            android:id="@+id/favourites"
    	            android:layout_width="wrap_content"
    	            android:layout_height="wrap_content"
    	            android:layout_marginLeft="200dp"
    	            android:layout_marginTop="50dp"
    	            android:layout_x="180dp"
    	            android:layout_y="36dp"
    	            android:src="@drawable/blue_favorites_b_150" />

    	        <ImageView
    	            android:id="@+id/social"
    	            android:layout_width="wrap_content"
    	            android:layout_height="wrap_content"
    	            android:layout_marginLeft="60dp"
    	            android:layout_marginTop="180dp"
    	            android:src="@drawable/blue_social_b_150" />

    	        <ImageView
    	            android:id="@+id/settings"
    	            android:layout_width="wrap_content"
    	            android:layout_height="wrap_content"
    	            android:layout_marginLeft="200dp"
    	            android:layout_marginTop="180dp"
    	            android:src="@drawable/blue_settings_b_150" />

    	      				  <ImageView
    	      				      android:id="@+id/tutorial"
    	      				      android:layout_width="wrap_content"
    	      				      android:layout_height="wrap_content"
    	      				      android:layout_marginLeft="10dp"
    	      				      android:layout_marginTop="295dp"
    	      				      android:src="@drawable/tutorial_icon" />
    	      				  
    	      				    <ImageView
    	      				        android:id="@+id/account"
    	      				        android:layout_width="wrap_content"
    	      				        android:layout_height="wrap_content"
    	      				        android:layout_marginLeft="250dp"
    	      				        android:layout_marginTop="295dp" />
    	        	
    	    </FrameLayout>

    	</SlidingDrawer>

    	<TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="100dp"
    	    android:layout_gravity="bottom"
    	    android:background="@drawable/drawerbuttonbackground" >

    		<ImageView
    		    android:id="@+id/feedback"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:layout_gravity="center"
    		    android:layout_marginLeft="40dp"
    		    android:src="@drawable/chat_icon_dashboard" />

    		<ImageView
    		    android:id="@+id/about"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:layout_gravity="center"
    		    android:layout_marginLeft="19dp"
    		    android:src="@drawable/aboutus_icon" />


    	      




    		<ImageView
    		    android:id="@+id/info"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:layout_gravity="center"
    		    android:layout_marginLeft="19dp"
    		    android:src="@drawable/twitter_icon" />



    		<ImageView
    		    android:id="@+id/exit"
    		    android:layout_width="wrap_content"
    		    android:layout_height="wrap_content"
    		    android:layout_gravity="center"
    		    android:layout_marginLeft="19dp"
    		    android:src="@drawable/exit_icon_dashboard" />
    		
    		
    		
    		

    	</TableRow>

    	

    	</FrameLayout>

<FrameLayout
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_gravity="bottom" >
    	
     <TextView 
    	           android:id="@+id/streetname"
    	           android:layout_width="wrap_content"
    	           android:layout_height="wrap_content"
    	           android:layout_gravity="center"
    			   android:textColor="#00000000"
    	           android:text=""
    	           />

     <ImageView
         android:id="@+id/arrow"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="19dp"
         android:visibility="gone" />

</FrameLayout>

   </FrameLayout>